﻿<div class="card">
    <div class="card-body">
        <div class="card-content">
            <h5 class="card-title">Brand: @bluetoothHeadphones.brand</h5>
            <h5 class="card-title">Model: @bluetoothHeadphones.model</h5>
            <h5 class="card-title">Color: @bluetoothHeadphones.color</h5>
            <h5 class="card-title">Frequency Range: @bluetoothHeadphones.frequencyRange</h5>
            <h5 class="card-title">Microphone: @bluetoothHeadphones.microphone</h5>
            <!--<h5 class="card-title">Noise Cancellation: @bluetoothHeadphones.noiseCancellation</h5>
            <h5 class="card-title">Closed / Open: @bluetoothHeadphones.closeOrOpen</h5>
            <h5 class="card-title">Condition: @bluetoothHeadphones.itemCondition</h5>-->
            <h4 class="card-title"><a href="@bluetoothHeadphones.Url">Click here to see the headset!</a></h4>
        </div>
    </div>
</div>

@code {
    [Parameter]
    public BluetoothHeadphones bluetoothHeadphones { get; set; }
}
